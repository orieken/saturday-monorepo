{
  "name": "@orieken/saturday-k6-redaction-basic",
  "version": "0.1.0",
  "description": "Basic redaction policy for @saturday/playwright-k6-exporter (headers, tokens, passwords).",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "TODO.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --out-dir dist",
    "clean": "rimraf dist",
    "prepack": "npm run clean && npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.30",
    "jest": "^30.2.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.6",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  }
}